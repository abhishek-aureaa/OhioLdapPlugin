							for (z = 1; z < iValue; z++)
							{
								printf(", %s", ppValue[z]);
							}
						}
					}
			}//end of if
					// Free memory.
					if (ppValue != NULL)
						ldap_value_free(ppValue);
					ppValue = NULL;
					ldap_memfree(pAttribute);
	
					// Get next attribute name.
					pAttribute = ldap_next_attribute(
